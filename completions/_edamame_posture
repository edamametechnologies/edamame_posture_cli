;;
(background-get-sessions)
_arguments "${_arguments_options[@]}" : \
'--zeek-format[Output sessions in Zeek format]' \
'--include-local-traffic[Include local traffic in the output]' \
'--fail-on-whitelist[Exit with code 1 if whitelist violations are detected]' \
'--fail-on-blacklist[Exit with code 1 if blacklisted sessions are detected]' \
'--fail-on-anomalous[Exit with code 1 if anomalous sessions are detected]' \
'*-v[Verbosity level (-v\: info, -vv\: debug, -vvv\: trace)]' \
'*--verbose[Verbosity level (-v\: info, -vv\: debug, -vvv\: trace)]' \
'-h[Print help]' \
'--help[Print help]' \
&& ret=0
;;
