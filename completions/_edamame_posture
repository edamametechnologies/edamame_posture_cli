'-h[Print help]' \
'--help[Print help]' \
'::ZEEK_FORMAT -- Zeek format:(true false)' \
'::LOCAL_TRAFFIC -- Include local traffic:(true false)' \
&& ret=0
;;
(background-threats-info)
_arguments "${_arguments_options[@]}" : \
'*-v[Verbosity level (-v\: info, -vv\: debug, -vvv\: trace)]' \
'*--verbose[Verbosity level (-v\: info, -vv\: debug, -vvv\: trace)]' \
'-h[Print help]' \
'--help[Print help]' \
&& ret=0
;;
(foreground-start)
_arguments "${_arguments_options[@]}" : \
'-u+[User name]:USER:_default' \
'--user=[User name]:USER:_default' \
'-d+[Domain name]:DOMAIN:_default' \
'--domain=[Domain name]:DOMAIN:_default' \
'-p+[PIN]:PIN:_default' \
'--pin=[PIN]:PIN:_default' \
'-k+[EDAMAME Portal LLM API key for AI assistant (edamame provider)]:API_KEY:_default' \
'--llm-api-key=[EDAMAME Portal LLM API key for AI assistant (edamame provider)]:API_KEY:_default' \
'--device-id=[Device ID suffix to flag the endpoint as a CI/CD runner when provided]:DEVICE_ID:_default' \
'--whitelist=[Whitelist name to enforce during capture]:WHITELIST:_default' \
'--agentic-mode=[AI assistant mode\: auto, analyze or disabled]:MODE:(auto analyze disabled)' \
'--agentic-provider=[LLM provider\: edamame (Portal), claude, openai, ollama, none]:PROVIDER:(edamame claude openai ollama none)' \
'--agentic-interval=[Interval in seconds for automated todo processing (default\: 3600)]:SECONDS:_default' \
'-n[Enable LAN scanning]' \
'--network-scan[Enable LAN scanning]' \
'-c[Enable packet capture]' \
'--packet-capture[Enable packet capture]' \
'--fail-on-whitelist[Treat whitelist violations as fatal (defaults to true when --whitelist is provided)]' \
'--fail-on-blacklist[Treat blacklist violations as fatal]' \
'--fail-on-anomalous[Treat anomalous sessions as fatal]' \
'--include-local-traffic[Include local traffic in capture output]' \
'--cancel-on-violation[Attempt to cancel the current CI pipeline when policy violations are detected]' \
'*-v[Verbosity level (-v\: info, -vv\: debug, -vvv\: trace)]' \
'*--verbose[Verbosity level (-v\: info, -vv\: debug, -vvv\: trace)]' \
'-h[Print help]' \
'--help[Print help]' \
&& ret=0
;;
(background-start)
_arguments "${_arguments_options[@]}" : \
